   "source": [
    "print(\"This algorithm calculates factorials. Enter your number.\")\n",
    "n = int(input())\n",
    "f = n*(n-1)\n",
    "for d in range (n-2, 1, -1):\n",
    "    f = f*d\n",
    "print(str(n)+\"! = \"+str(f)+\".\")"
   ]
