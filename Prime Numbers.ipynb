    "print(\"This algorithm checks if a number is prime. Enter your number.\")\n",
    "n = int(input())\n",
    "d = 2\n",
    "while n>d:\n",
    "    if n%d==0:\n",
    "        print(str(n)+\" is not prime.\")\n",
    "        break\n",
    "    else:\n",
    "        d = d+1\n",
    "print(str(n)+\" is prime.\")\n",
